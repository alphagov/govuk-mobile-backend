  "scripts": {
    "test": "nx run-many --target test",
    "test:unit": "nx run-many --target=test:unit",
    "test:coverage": "vitest run --coverage --project unit",
    "test:acc": "nx run-many --target=test:acc",
    "test:int": "nx run-many --target=test:int",
    "lint": "nx run-many --target lint",
    "sam:build:affected": "node ./scripts/samBuild.js --projects=affected",
    "sam:build:all": "node ./scripts/samBuild.js --projects=all",
    "sam:validate:affected": "node ./scripts/samValidate.js --projects=affected",
    "sam:validate:all": "node ./scripts/samValidate.js --projects=all",
    "sam:deploy:affected": "npm run sam:build:affected && ./scripts/samDeploy.js --projects=affected",
    "sam:deploy:all": "npm run sam:build:affected && node ./scripts/samDeploy.js --projects=all",
    "commit": "cz commit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },

{
    "name": "auth",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "targets": {
        "install": {
            "cache": true,
            "executor": "nx:run-commands",
            "options": {
                "command": "npm i",
                "cwd": "auth"
            }
        },
        "build": {
            "cache": true,
            "executor": "nx:run-commands",
            "outputs": [
                "{options.outputFile}"
            ],
            "dependsOn": [
                "install"
            ],
            "options": {
                "command": "npm i && npm run compile",
                "cwd": "auth/hello-world"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "**/*.ts"
                ]
            }
        },
        "sam:lint": {
            "executor": "nx:run-commands",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "command": "npm run sam:lint",
                "cwd": "auth"
            }
        },
        "sam:validate": {
            "executor": "nx:run-commands",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "command": "npm run sam:validate",
                "cwd": "auth"
            }
        },
        "test:unit": {
            "dependsOn": [
                "install"
            ],
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run test:unit",
                "cwd": "auth"
            }
        },
        "test:acc": {
            "dependsOn": [
                "install"
            ],
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run --project acc",
                "cwd": "auth"
            },
            "outputs": [
                "{options.outputFile}"
            ]
        },
        "test:functional": {
            "dependsOn": [
                "install"
            ],
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run test:acc",
                "cwd": "auth"
            },
            "outputs": [
                "{options.outputFile}"
            ]
        },
        "test:coverage": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run test:coverage",
                "cwd": "auth"
            }
        }
    }
}
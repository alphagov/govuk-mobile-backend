    "@types/node-fetch": "^2.6.12",
    "@types/node-jose": "^1.1.13",
    "node-jose": "^2.2.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }

    "@aws-sdk/client-cloudwatch-logs": "^3.850.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.850.0",
    "@aws-sdk/client-iam": "^3.850.0",
    "@aws-sdk/client-lambda": "^3.850.0",
    "@aws-sdk/client-secrets-manager": "^3.850.0",
    "@aws-sdk/client-sns": "^3.850.0",
    "@aws-sdk/client-ssm": "^3.850.0",
    "aws-jwt-verify": "^5.1.0",
    "aws-lambda": "^1.0.0",
    "axios": "^1.11.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.9",
    "http-status-codes": "^2.3.0",
    "jose": "^6.0.12",
